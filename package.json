    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "build:client-and-server-bundles": "ng build --prod && ng run ngbe2018:server",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
